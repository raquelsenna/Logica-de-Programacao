algoritmo "PROCEDIMENTO_3"
var
   x, y: inteiro
   Procedimento soma (var a, b: inteiro)
   inicio
         a <- a + 1
         b <- b + 3
         Escreval ("O valor de A e ", a)
         Escreval ("O valor de B e ", b)
   FimProcedimento
inicio
      x <- 5
      y <- 9
      soma (x,y)
      Escreval ("O valor de X e ", x)
      Escreval ("O valor de Y e ", y)
fimalgoritmo